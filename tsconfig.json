{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "types": ["datatables.net-bs5", "datatables.net-autofill", "datatables.net-autofill-bs5", "datatables.net-buttons", "datatables.net-buttons-bs5"]
  },
  "include": ["src"]
}
